{
  "name": "meeting-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "nodemon": "nodemon --legacy-watch -w src/ src/server.js",
    "build": "vite build",
    "build:extension": "vite build --mode extension",
    "preview": "vite preview",
    "dev": "vite",
    "dev:server": "nodemon --legacy-watch -w src/ src/server.js",
    "dev:extension": "concurrently \"npm run dev:server\" \"npm run build:extension -- --watch\"",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "pack:extension": "npm run build:extension && cd dist/extension && zip -r meeting-assistant-extension.zip ."
  },
  "dependencies": {
    "@google-cloud/speech": "^7.2.0",
    "@preact/preset-vite": "^2.10.2",
    "@tailwindcss/vite": "^4.1.11",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.2",
    "openai": "^5.10.2",
    "preact": "^10.27.1",
    "tailwindcss": "^4.1.11",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-static-copy": "^3.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.1.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "vite": "^7.0.4"
  }
}
